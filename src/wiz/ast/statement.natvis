    <Expand>
      <Item Name="[kind]">kind</Item>
      <Item Name="[attribution]" Condition="kind == wiz::StatementKind::Attribution">attribution</Item>
      <Item Name="[bank]" Condition="kind == wiz::StatementKind::Bank">bank</Item>
