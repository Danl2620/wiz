      <Item Name="[expressionStatement]" Condition="kind == wiz::StatementKind::ExpressionStatement">expressionStatement</Item>
      <Item Name="[file]" Condition="kind == wiz::StatementKind::File">file</Item>
      <Item Name="[for_]" Condition="kind == wiz::StatementKind::For">for_</Item>
      <Item Name="[func]" Condition="kind == wiz::StatementKind::Func">func</Item>
      <Item Name="[if_]" Condition="kind == wiz::StatementKind::If">if_</Item>
